{
    "name": "Color Picker",
    "description":"Pick color from any website",
    "version" :"1.0.0",
    "manifest_version": 3,
    "background":{
        "service_worker": "background.js"
    },
    "permissions":["activeTab","scripting"],
    "action":{
        "default_popup":"popup.html"
    },
    "icons":{
        "16":"/assests/color-picker_16.png",
        "32":"assests/color-picker_32.png",
        "64":"assests/color-picker_64.png"
    }
}